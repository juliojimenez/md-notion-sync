{
  "name": "md-notion-sync",
  "version": "1.0.1",
  "description": "Sync markdown files to Notion pages.",
  "keywords": [
    "notion",
    "markdown",
    "sync",
    "documentation",
    "readme"
  ],
  "homepage": "https://github.com/juliojimenez/md-notion-sync#readme",
  "bugs": {
    "url": "https://github.com/juliojimenez/md-notion-sync/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/juliojimenez/md-notion-sync.git"
  },
  "license": "Apache-2.0",
  "author": "Julio Jimenez <julio@julioj.com>",
  "type": "commonjs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "md-notion-sync": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest --forceExit --detectOpenHandles --verbose",
    "test:watch": "jest --watch --forceExit",
    "test:coverage": "jest --coverage --forceExit --detectOpenHandles",
    "test:ci": "jest --ci --coverage --watchAll=false --forceExit",
    "test:simple": "node scripts/run-tests.js",
    "prepublishOnly": "npm run build && npm run test:ci",
    "lint": "eslint src/**/*.ts tests/**/*.ts",
    "lint:fix": "eslint src/**/*.ts tests/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts tests/**/*.ts"
  },
  "dependencies": {
    "@notionhq/client": "^4.0.2",
    "commander": "^11.0.0",
    "dotenv": "^17.2.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/node": "^20.5.0",
    "@typescript-eslint/eslint-plugin": "^6.4.0",
    "@typescript-eslint/parser": "^8.40.0",
    "eslint": "^8.47.0",
    "jest": "^29.6.2",
    "prettier": "^3.0.2",
    "ts-jest": "^29.1.1",
    "typescript": "^5.1.6"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=16.0.0"
  }
}
